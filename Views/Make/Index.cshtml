@model IEnumerable<BykeSellingOnlinePlatefrom.Models.Make>
@{
    ViewData["Title"] = "Index";
}


<div class="row">
    <div class="col-6 ">
        <h2 class="text-primary align-content-center floa" >Make Vechical</h2>
    </div>
    <div class="col-6">
        <a class="btn btn-primary float-end m-lg-4" asp-action="Create"><i class="fa fa-motorcycle" aria-hidden="true"></i> &nbsp; Add Make</a>
    </div>
</div>


<div>

    <table class="table table-dark table-striped">
        <thead class="table-info">
            <tr>
                <th>@Html.DisplayNameFor(m=>m.Name)</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(m=>item.Name)</td>
                    <partial name="_TablebuttonPartial" model="@item.Id"></partial>
                    
                </tr>
            }

        </tbody>
    </table>

</div>


